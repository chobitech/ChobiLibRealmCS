<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0;netstandard2.1</TargetFrameworks>
    <LangVersion>12.0</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
      <PackageId>ChobiLibRealmCS</PackageId>
      <Version>0.0.3</Version>
      <PackageReadmeFile>README.md</PackageReadmeFile>
      <Authors>chobitech</Authors>
      <Company>chobitech</Company>
      <Copyright>© 2025 chobitech. All rights reserved.</Copyright>
      <Description>C# support library on Realm for chobitech use</Description>
      <PackageLicenseExpression>MIT</PackageLicenseExpression>
      <RepositoryUrl>https://github.com/chobitech/ChobiLibRealmCS</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Realm" Version="20.1.0" />
  </ItemGroup>

</Project>
